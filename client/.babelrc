{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "react-hot-loader/babel",
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/transform-runtime"]
  ],
  "env": {
    "production": {
      "presets": [
        "minify"
      ]
    }
  }
}
